---
import Layout from "../components/layouts/base.astro";
const imgs = await Astro.glob("../assets/img/ai/lg/*.webp").then((files) => {
    return files.map((file) => file.default);
});
import { onlyFileName } from "~/utils";
---
<Layout>

        {
            imgs.map((img) => (
                    <a name={onlyFileName(img.src)} />
                    <img
                        src={img.src}
                        loading="lazy"
                        width={img.width}
                        alt={img.src}
                        title={img.src}
                    />

            ))
        }
</Layout>



