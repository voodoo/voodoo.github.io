---
import Logo from "~/components/logo.astro";
import Layout from "../components/layouts/base.astro";
const imgs = await Astro.glob("../assets/img/ai/lg/*.webp").then((files) => {
    return files.map((file) => file.default);
});

---
<Layout>

        {
            imgs.map((img) => (
                <>
                    <a
                        name={img.src
                            .substring(img.src.lastIndexOf("/"))
                            .replace("/", "")}
                    />
                    <img
                        src={img.src}
                        loading="lazy"
                        style="width:100vw;margin:0"
                        alt=""
                    />
                </>
            ))
        }
</Layout>
